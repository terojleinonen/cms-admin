{
  "name": "kin-workspace-cms",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "db:setup": "./scripts/setup-database.sh",
    "db:setup:production": "./scripts/setup-production-database.sh",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "db:push": "prisma db push",
    "db:migrate-ecommerce": "tsx scripts/migrate-ecommerce-data.ts",
    "db:backup": "./scripts/database-backup.sh",
    "db:backup:full": "./scripts/database-backup.sh full",
    "db:backup:schema": "./scripts/database-backup.sh schema",
    "db:backup:data": "./scripts/database-backup.sh data",
    "db:restore": "./scripts/database-restore.sh",
    "db:health": "curl -s http://localhost:3001/api/health | jq '.checks[0].details'",
    "db:test-health": "npx tsx scripts/test-database-health.ts",
    "perf:analyze": "ANALYZE=true npm run build",
    "install:ci": "npm ci --legacy-peer-deps",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:e2e": "jest --selectProjects e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "deps:audit": "npm audit --audit-level=moderate",
    "deps:validate": "npm run deps:audit",
    "ci:validate": "npm run lint && npm run type-check && npm run deps:validate && npm run test:ci"
  },
  "dependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.13.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/dompurify": "^3.0.5",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "babel-jest": "^30.1.2",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "isomorphic-dompurify": "^2.28.0",
    "jsonwebtoken": "^9.0.2",
    "next": "^15.1.0",
    "next-auth": "^4.24.8",
    "nodemailer": "^7.0.10",
    "postcss": "^8.5.1",
    "prisma": "^6.13.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "redis": "^5.8.3",
    "sharp": "^0.34.3",
    "tailwindcss": "^4.1.13",
    "uuid": "^11.1.0",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@babel/preset-flow": "^7.27.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@faker-js/faker": "^9.9.0",
    "@tailwindcss/upgrade": "^4.0.6",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.1",
    "@types/nodemailer": "^7.0.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/sharp": "^0.31.1",
    "commander": "^12.1.0",
    "eslint": "^9.35.0",
    "eslint-config-next": "^15.5.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "glob": "^10.4.5",
    "globals": "^16.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "jest-environment-node": "^30.0.5",
    "jiti": "^2.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.3",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.42.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "overrides": {
    "glob": "^10.4.5"
  }
}
